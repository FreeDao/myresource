package com.realtek.bitmapfun.util;

public class PicSize
    {
    	private int height;
    	private int width;
    	public PicSize(int h,int w)
    	{
    		height = h;
    		width = w;
    	}
    	public int getHeight()
    	{
    		return height;
    	}
    	public int getWidth()
    	{
    		return width;
    	}
    }