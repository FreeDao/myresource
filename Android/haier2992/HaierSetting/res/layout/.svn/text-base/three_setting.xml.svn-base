                android:textColor="#CCCCCCCC"
                android:textSize="22sp" />

            <ImageView
                android:id="@+id/nine_right_arrowhead"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
