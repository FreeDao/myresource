                android:layout_centerVertical="true"
                android:layout_marginRight="50dip"
                android:layout_toLeftOf="@id/four_right_arrowhead"
                android:background="@drawable/one_px"
                android:text="@string/close"
                android:textColor="#CCCCCCCC"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/four_left_arrowhead"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="50dip"
                android:layout_toLeftOf="@id/d3_to_2d_btn"
                android:background="@drawable/desktop_left"
