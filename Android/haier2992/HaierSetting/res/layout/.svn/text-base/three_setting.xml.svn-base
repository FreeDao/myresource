                android:layout_centerVertical="true"
                android:layout_marginRight="50dip"
                android:layout_toLeftOf="@id/three_right_arrowhead"
                android:background="@drawable/one_px"
                android:text="@string/close"
                android:textColor="#CCCCCCCC"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/three_left_arrowhead"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="50dip"
                android:layout_toLeftOf="@id/d3_mode_btn"
                android:background="@drawable/desktop_left"
                android:visibility="invisible" />
        </RelativeLayout>

        <RelativeLayout
