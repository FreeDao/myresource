<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical"    android:background="@drawable/list_common_bg"    android:id="@+id/main"    tools:context=".VideoBrowser" >        <RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent" >	    <RelativeLayout	            android:id="@+id/bg_bottom"	            android:layout_width="960dp"	            android:layout_height="34.5dp"	            android:layout_marginLeft="0dp"	            android:layout_marginTop="515.5dp"	            android:background="@drawable/list_common_bg_bottom">	        <RelativeLayout	            android:id="@+id/guide_center"	            android:layout_width="wrap_content"	            android:layout_height="wrap_content"	            android:layout_centerHorizontal="true"	            android:layout_centerVertical="true" >	            <TextView	                android:id="@+id/guide_quick"	                android:layout_width="wrap_content"	                android:layout_height="wrap_content"	                android:background="@drawable/hint_black"	                android:gravity="center"	                android:paddingLeft = "4dp"	                android:paddingRight = "4dp"	                android:text="@string/guide_quick"	                android:textColor="@color/white"	                android:textSize="15sp" />	            <TextView	                android:id="@+id/guide_features"	                android:layout_width="wrap_content"	                android:layout_height="wrap_content"	                android:layout_marginLeft="6dp"	                android:layout_toRightOf="@id/guide_quick"	                android:gravity="center"	                android:text="@string/guide_features"	                android:textColor="#4A4A4A"	                android:textSize="15sp" />	        </RelativeLayout>	        <RelativeLayout	            android:id="@+id/guide_left"	            android:layout_width="wrap_content"	            android:layout_height="wrap_content"	            android:layout_centerVertical="true"	            android:layout_marginRight="21dp"	            android:layout_toLeftOf="@id/guide_center" >	            <TextView	                android:id="@+id/guide_ok"	                android:layout_width="wrap_content"	                android:layout_height="wrap_content"	                android:paddingLeft = "4dp"	                android:paddingRight = "4dp"	                android:background="@drawable/hint_black"	                android:gravity="center"	                android:text="@string/guide_ok"	                android:textColor="@color/white"	                android:textSize="15sp" />	            <TextView	                android:id="@+id/guide_enter"	                android:layout_width="wrap_content"	                android:layout_height="wrap_content"	                android:layout_marginLeft="6dp"	                android:layout_toRightOf="@id/guide_ok"	                android:gravity="center"	                android:text="@string/guide_enter"	                android:textColor="#4A4A4A"	                android:textSize="15sp" />	        </RelativeLayout>	        <RelativeLayout	            android:id="@+id/guide_right"	            android:layout_width="wrap_content"	            android:layout_height="wrap_content"	            android:layout_centerVertical="true"	            android:layout_marginLeft="21dp"	            android:layout_toRightOf="@id/guide_center" >	            <TextView	                android:id="@+id/guide_p"	                android:layout_width="wrap_content"	                android:layout_height="wrap_content"	                android:paddingLeft = "4dp"	                android:paddingRight = "4dp"	                android:background="@drawable/hint_black"	                android:gravity="center"	                android:text="@string/guide_p"	                android:textColor="@color/white"	                android:textSize="15sp" />	            <TextView	                android:id="@+id/guide_page"	                android:layout_width="wrap_content"	                android:layout_height="wrap_content"	                android:layout_marginLeft="6dp"	                android:layout_toRightOf="@id/guide_p"	                android:gravity="center"	                android:text="@string/guide_page"	                android:textColor="#4A4A4A"	                android:textSize="15sp" />	        </RelativeLayout>	    </RelativeLayout>	    <RelativeLayout	            android:id="@+id/resume_play_help_bar"	            android:layout_width="960dp"	            android:layout_height="34.5dp"	            android:layout_marginLeft="0dp"	            android:layout_marginTop="515.5dp"	            android:visibility="invisible"	            android:background="@drawable/list_common_bg_bottom">	        	<RelativeLayout		            android:id="@+id/guide_left1"		            android:layout_width="wrap_content"		            android:layout_height="wrap_content"		            android:layout_centerVertical="true"		            android:layout_marginLeft="394.5dp"	            	android:layout_marginTop="6.75dp">	            	<TextView			            android:id="@+id/guide_arrows"			            android:layout_width="34.5dp"			            android:layout_height="21dp"			            android:paddingLeft = "4dp"		                android:paddingRight = "4dp"			            android:background="@drawable/guide_csl_rl"			            android:gravity="center" />	            	<TextView			            android:id="@+id/guide_select"			            android:layout_width="wrap_content"			            android:layout_height="wrap_content"			            android:layout_marginLeft="6dp"			            android:layout_toRightOf="@+id/guide_arrows"			            android:textSize="15sp"			            android:gravity="center"			            android:textColor="#4A4A4A"			            android:text="@string/guide_select" />	            </RelativeLayout>	            <RelativeLayout		            android:id="@+id/guide_right1"		            android:layout_width="wrap_content"		            android:layout_height="wrap_content"		            android:layout_centerVertical="true"		            android:layout_marginLeft="21dp"		            android:layout_toRightOf="@id/guide_left1" >	            	<TextView			            android:id="@+id/guide_ok1"			            android:layout_width="wrap_content"			            android:layout_height="wrap_content"			            android:background="@drawable/hint_black"			            android:gravity="center"			            android:paddingLeft = "4dp"	                	android:paddingRight = "4dp"	                	android:textSize="15sp"			            android:text="@string/guide_ok"			            android:textColor="@color/white" />	            	<TextView			            android:id="@+id/guide_enter1"			            android:layout_width="wrap_content"			            android:layout_height="wrap_content"			            android:layout_toRightOf="@+id/guide_ok1"			            android:layout_marginLeft="6dp"			            android:textSize="15sp"			            android:gravity="center"			             android:background="@drawable/hint_black"			            android:textColor="@color/white"			            android:text="@string/guide_enter" />	     		</RelativeLayout>	    </RelativeLayout>	    <ImageView	        android:id="@+id/focus"	        android:layout_width="502.5dp"	        android:layout_height="33.9975dp"	        android:layout_marginLeft="0dp"	        android:layout_marginTop="0dp"	        android:scaleType="fitCenter"	        android:src="@drawable/list_common_item_focus"	        android:visibility="gone" />	    <SurfaceView        	android:id="@+id/video"        	android:layout_width="225dp"        	android:layout_height="127.5dp"        	android:layout_marginLeft="652dp"            android:layout_marginTop="142dp"            android:scaleType="fitCenter" />	    <ProgressBar        	android:id="@+id/video_progress"       	 	style="?android:attr/progressBarStyleHorizontal"        	android:layout_width="225dp"        	android:layout_height="6dp"        	android:layout_marginLeft="652.5dp"        	android:layout_marginTop="133dp"        	android:progressDrawable="@drawable/listprocess"/>	    <TextView	        android:id="@+id/dir_or_video_name"	        android:layout_width="225dp"	        android:layout_height="27dp"	        android:layout_marginLeft="652.5dp"	        android:layout_marginTop="275dp"	        android:gravity="left|center_vertical"	        android:shadowColor="#aaaaaa"	        android:textColor="#FFFFFF"	        android:singleLine="true"	        android:textSize="12sp" />	    <ProgressBar	        android:id="@+id/loadingIcon"	        style="@android:attr/progressBarStyleSmall"	        android:layout_width="27dp"	        android:layout_height="27dp"	        android:layout_marginLeft="904.5dp"	        android:layout_marginTop="27dp"	        android:indeterminateDrawable="@anim/video_anim"	      />	    <ImageView	        android:id="@+id/image_folder"	        android:layout_width="225dp"	        android:layout_height="127.5dp"	        android:layout_marginLeft="652.5dp"	        android:layout_marginTop="142.5dp" />	    <ImageView	        android:id="@+id/video_list_icon_video"	        android:layout_width="wrap_content"	        android:layout_height="wrap_content"	        android:layout_marginLeft="55dp"	        android:layout_marginTop="14.25dp"	        android:src="@drawable/video_list_icon_video1" />	    <TextView	        android:id="@+id/Top_VideoText"	        android:layout_toRightOf="@+id/video_list_icon_video"        	android:layout_alignTop="@+id/video_list_icon_video"        	android:layout_marginTop="4.5dp"        	android:layout_marginLeft="11.25dp"	        android:layout_width="wrap_content"	        android:layout_height="28.5dp"	        android:text="@string/title_movie"	        android:textColor="#FFFFFF"	        android:textSize="22.5dp" />	    <ImageView	        android:id="@+id/video_list_icon_line"	        android:layout_width="510dp"	        android:layout_height="wrap_content"	        android:layout_marginLeft="67.5dp"	        android:layout_marginTop="112dp"	        />	    <TextView 	        android:id="@+id/top_divid_line"	        android:layout_width="502dp"	        android:layout_height="0.5025dp"	        android:layout_marginLeft="67.5dp"	        android:layout_marginTop="110dp"	        android:background="@color/white"	        />	    <ImageView	        android:id="@+id/video_list_icon_folder"	        android:layout_width="wrap_content"	        android:layout_height="wrap_content"	        android:layout_marginLeft="62.2dp"	        android:layout_marginTop="81dp"	        android:src="@drawable/list_common_icon_folder" />	    <TextView	        android:id="@+id/video_path_top"	        android:layout_width="360dp"	        android:layout_height="wrap_content"	        android:layout_toRightOf="@+id/video_list_icon_folder"	        android:layout_marginLeft="2dp"	        android:layout_marginTop="82.5dp"	        android:shadowColor="#aaaaaa"	        android:singleLine="true"	        android:textColor="#FFFFFF"	        android:ellipsize="end"	        android:textSize="15sp"/>	    <TextView	        android:id="@+id/video_list_total_time_top"	        android:layout_width="175dp"            android:layout_height="22.5dp"            android:layout_marginLeft="448dp"            android:layout_marginTop="84dp"            android:gravity="center"            android:shadowColor="#aaaaaa"            android:singleLine="true"            android:text="@string/totaltime"            android:textColor="#FFFFFF"            android:textSize="15sp"/>	    <TextView	        android:id="@+id/video_list_index_info"	        android:layout_width="90dp"	        android:layout_height="22.5dp"	        android:layout_marginLeft="787.5dp"	        android:layout_marginTop="86.25dp"	        android:gravity="right"	        android:shadowColor="#aaaaaa"	        android:textColor="#FFFFFF"	        android:textSize="15sp"/>	    <ListView	        android:id="@+id/video_list"	        android:layout_width="502.5dp"	        android:layout_height="380dp"	        android:layout_marginLeft="67.5dp"	        android:layout_marginTop="112.5dp"	        android:divider="@color/ListViewDivideLine"	        android:dividerHeight="0.5025dp"	        android:focusable="true"            android:focusableInTouchMode="true"	        android:listSelector="@drawable/selector_list_background"	        android:scrollbars="none" />	    <ImageView	        android:id="@+id/video_list_page_up"	        android:layout_width="wrap_content"	        android:layout_height="wrap_content"	        android:layout_marginLeft="591dp"	        android:layout_marginTop="97.5dp"	        android:scaleType="fitCenter"	        android:src="@drawable/list_common_icon_up" />	    <ImageView	        android:id="@+id/video_list_page_down"	        android:layout_width="wrap_content"	        android:layout_height="wrap_content"	        android:layout_marginLeft="591dp"	        android:layout_marginTop="472.5dp"	        android:scaleType="fitCenter"	        android:src="@drawable/list_common_icon_down" />	    <ImageView	        android:id="@+id/imgRepeat"	        android:layout_width="22.5dp"	        android:layout_height="22.5dp"	        android:layout_marginLeft="857dp"	        android:layout_marginTop="111dp"	        android:background="#00000000"	        android:scaleType="fitCenter"	        android:src="@drawable/photo_repeat_all_on" />	    <TextView	        android:id="@+id/date"	        android:layout_width="270dp"	        android:layout_height="27dp"	        android:layout_marginLeft="652.5dp"	        android:layout_marginTop="302dp"	        android:gravity="left|center_vertical"	        android:shadowColor="#aaaaaa"	        android:singleLine="true"	        android:textColor="#FFFFFF"	        android:textSize="12sp" />	    <TextView	        android:id="@+id/video_hasplayed"	        android:layout_width="wrap_content"	        android:layout_height="wrap_content"	        android:layout_marginLeft="652.5dp"	        android:layout_marginTop="118dp"	        android:textColor="#FFFFFF"	        android:textSize="12sp" />	    <TextView	        android:id="@+id/video_duration_tag"	        android:layout_width="wrap_content"	        android:layout_height="wrap_content"	        android:layout_toRightOf="@+id/video_hasplayed"	        android:layout_marginLeft="2dp"	        android:layout_marginTop="118dp"	        android:gravity="center"	        android:text="@string/gapline"	        android:textColor="@color/white"	        android:textSize="12sp" />	    <TextView	        android:id="@+id/video_duration"	        android:layout_width="wrap_content"	        android:layout_height="wrap_content"	        android:layout_toRightOf="@+id/video_duration_tag"	        android:layout_marginLeft="2dp"	        android:layout_marginTop="118dp"	        android:textColor="@color/white"	        android:textSize="12sp" />    </RelativeLayout>    </LinearLayout>