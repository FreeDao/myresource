<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/list_common_bg"
    android:orientation="vertical" >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
        <ImageView
	        android:id="@+id/focus"
	        android:layout_width="670dp"
	        android:layout_height="45.33dp"
	        android:layout_marginLeft="0dp"
	        android:layout_marginTop="0dp"
	        android:scaleType="fitCenter"
	        android:src="@drawable/list_common_item_focus"
	        android:visibility="gone" />
        <LinearLayout
            android:id="@+id/bg_bottom"
            android:layout_width="1280dp"
            android:layout_height="46dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="674dp"
            android:background="@drawable/list_common_bg_bottom" />
        <ImageView
            android:id="@+id/audio_image"
            android:layout_width="300dp"
            android:layout_height="170dp"
            android:layout_marginLeft="870dp"
            android:layout_marginTop="190dp"
            android:scaleType="fitCenter" />
        <ProgressBar
            android:id="@+id/loadingIcon"
            style="@android:attr/progressBarStyleSmall"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginLeft="1206dp"
            android:layout_marginTop="36dp"
            android:background="#00000000"
            android:indeterminateDrawable="@anim/video_anim" />
        <TextView
            android:id="@+id/audio_artist"
            android:layout_width="294dp"
            android:layout_height="24dp"
            android:layout_marginLeft="878dp"
            android:layout_marginTop="388dp"
            android:ellipsize="end"
            android:gravity="left|center_vertical"
            android:singleLine="true"
            android:textColor="#0f265f"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/audio_album"
            android:layout_width="294dp"
            android:layout_height="24dp"
            android:layout_marginLeft="878dp"
            android:layout_marginTop="412dp"
            android:ellipsize="end"
            android:gravity="left|center_vertical"
            android:singleLine="true"
            android:textColor="#0f265f"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/audio_path"
            android:layout_width="352dp"
            android:layout_height="36dp"
            android:layout_marginLeft="128dp"
            android:layout_marginTop="604dp"
            android:shadowColor="#aaaaaa"
            android:singleLine="true"
            android:textColor="#0f265f"
            android:textSize="18dp"
            android:visibility="gone" />
        <ProgressBar
            android:id="@+id/audio_progress"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="334dp"
            android:layout_height="8dp"
            android:layout_marginLeft="860dp"
            android:layout_marginTop="490dp"
            android:progressDrawable="@drawable/listprocess" />
        <TextView
            android:id="@+id/audio_hasplayed"
            android:layout_width="114dp"
            android:layout_height="16dp"
            android:layout_marginLeft="976dp"
            android:layout_marginTop="520dp"
            android:textColor="#0f265f"
            android:textSize="13dp" />
        <ListView
            android:id="@+id/audio_list"
            android:layout_width="670dp"
            android:layout_height="552dp"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="120dp"
            android:dividerHeight="0.67dp"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:listSelector="@drawable/selector_list_background"
            android:scrollbars="none" >
        </ListView>
        <ImageView
            android:id="@+id/page_up"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="788dp"
            android:layout_marginTop="132dp"
            android:scaleType="fitCenter"
            android:src="@drawable/list_common_icon_up" />
        <ImageView
            android:id="@+id/page_down"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="788dp"
            android:layout_marginTop="638dp"
            android:scaleType="fitCenter"
            android:src="@drawable/list_common_icon_down" />
        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="20dp"
            android:background="#00000000"
            android:src="@drawable/music_list_icon_music" />
        <ImageView
            android:id="@+id/imgPath"
            android:layout_width="54dp"
            android:layout_height="45.33dp"
            android:layout_marginLeft="84dp"
            android:layout_marginTop="76dp"
            android:scaleType="fitXY"
            android:src="@drawable/list_common_icon_folder" />
        <TextView
            android:id="@+id/textView1"
            android:layout_width="102dp"
            android:layout_height="38dp"
            android:layout_marginLeft="105dp"
            android:layout_marginTop="25dp"
            android:text="@string/title_music"
            android:textColor="#0f265f"
            android:textSize="30dp" />
        <TextView
            android:id="@+id/music_path_top"
            android:layout_width="480dp"
            android:layout_height="30dp"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="82dp"
            android:ellipsize="end"
            android:shadowColor="#aaaaaa"
            android:singleLine="true"
            android:textColor="#0f265f"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/totaltxt"
            android:layout_width="100dp"
            android:layout_height="30dp"
            android:layout_marginLeft="670dp"
            android:layout_marginTop="82dp"
            android:shadowColor="#aaaaaa"
            android:singleLine="true"
            android:text="@string/totaltime"
            android:textColor="#0f265f"
            android:textSize="20sp" />
        <ImageView
            android:id="@+id/audio_list_icon_line"
            android:layout_width="680dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="118dp"
            android:src="@drawable/list_common_icon_line" />
        <TextView
            android:id="@+id/audio_duration_tag"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginLeft="1020dp"
            android:layout_marginTop="520dp"
            android:gravity="center"
            android:text="@string/gapline"
            android:textColor="@color/black"
            android:textSize="13dp" />
        <TextView
            android:id="@+id/audio_duration"
            android:layout_width="114dp"
            android:layout_height="16dp"
            android:layout_marginLeft="1034dp"
            android:layout_marginTop="520dp"
            android:textColor="#0f265f"
            android:textSize="13dp" />
        <ImageButton
            android:id="@+id/imgRepeat"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginLeft="1148dp"
            android:layout_marginTop="520dp"
            android:background="#00000000"
            android:scaleType="fitCenter"
            android:src="@drawable/av_repeat_icon" />
        <TextView
            android:id="@+id/TopIndexInfo_audio"
            android:layout_width="152dp"
            android:layout_height="40dp"
            android:layout_marginLeft="734dp"
            android:layout_marginTop="78dp"
            android:gravity="center"
            android:shadowColor="#aaaaaa"
            android:textColor="#0f265f"
            android:textSize="20dp" />
	    <RelativeLayout
	            android:id="@+id/bg_bottom"
	            android:layout_width="1280dp"
	            android:layout_height="46dp"
	            android:layout_marginLeft="0dp"
	            android:layout_marginTop="674dp"
	            android:background="@drawable/list_common_bg_bottom">

	        <RelativeLayout
	            android:id="@+id/guide_center"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >

	            <TextView
	                android:id="@+id/guide_quick"
	                android:layout_width="70dp"
	                android:layout_height="28dp"
	                android:background="@drawable/guide_icon_bg"
	                android:gravity="center"
	                android:text="@string/guide_quick"
	                android:textColor="@color/white"
	                android:textSize="20sp" />

	            <TextView
	                android:id="@+id/guide_features"
	                android:layout_width="wrap_content"
	                android:layout_height="28dp"
	                android:layout_marginLeft="8dp"
	                android:layout_toRightOf="@id/guide_quick"
	                android:gravity="center"
	                android:text="@string/guide_features"
	                android:textColor="#0f265f"
	                android:textSize="20sp" />

	        </RelativeLayout>

	        <RelativeLayout
	            android:id="@+id/guide_left"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerVertical="true"
	            android:layout_marginRight="28dp"
	            android:layout_toLeftOf="@id/guide_center" >

	            <TextView
	                android:id="@+id/guide_ok"
	                android:layout_width="46dp"
	                android:layout_height="28dp"
	                android:background="@drawable/guide_icon_bg"
	                android:gravity="center"
	                android:text="@string/guide_ok"
	                android:textColor="@color/white"
	                android:textSize="20sp" />

	            <TextView
	                android:id="@+id/guide_enter"
	                android:layout_width="wrap_content"
	                android:layout_height="28dp"
	                android:layout_marginLeft="8dp"
	                android:layout_toRightOf="@id/guide_ok"
	                android:gravity="center"
	                android:text="@string/guide_enter"
	                android:textColor="#0f265f"
	                android:textSize="20sp" />

	        </RelativeLayout>

	        <RelativeLayout
	            android:id="@+id/guide_right"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerVertical="true"
	            android:layout_marginLeft="28dp"
	            android:layout_toRightOf="@id/guide_center" >

	            <TextView
	                android:id="@+id/guide_p"
	                android:layout_width="28dp"
	                android:layout_height="28dp"
	                android:background="@drawable/guide_icon_bg"
	                android:gravity="center"
	                android:text="@string/guide_p"
	                android:textColor="@color/white"
	                android:textSize="20sp" />

	            <TextView
	                android:id="@+id/guide_page"
	                android:layout_width="wrap_content"
	                android:layout_height="28dp"
	                android:layout_marginLeft="8dp"
	                android:layout_toRightOf="@id/guide_p"
	                android:gravity="center"
	                android:text="@string/guide_page"
	                android:textColor="#0f265f"
	                android:textSize="20sp" />

	        </RelativeLayout>

	    </RelativeLayout>
    </RelativeLayout>
</LinearLayout>
