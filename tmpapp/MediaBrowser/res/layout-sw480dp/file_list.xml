<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="410.5dp"
    android:layout_height="33.0dp"
    android:orientation="horizontal" >
    

<ImageView android:id="@+id/icon"
	android:layout_width="26.4dp"
	android:layout_height="23.1dp" 
	android:paddingLeft="6.6dp"
	android:paddingTop="6.6dp"
	/>

<TextView
    android:id="@+id/file_name"
    android:layout_width="357.1dp"
    android:layout_height="26.4dp"
    android:layout_gravity="top"
    android:gravity="center_vertical"
    android:singleLine="true"
    android:paddingTop="4.6dp"
    android:paddingLeft="6.6dp"
    android:shadowColor="#aaaaaa"
    android:textColor="#0f265f"
    android:textSize="13.2dp" />
	
</LinearLayout>
