<?xml version="1.0" encoding="utf-8"?><AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/image_bw_thumb_bg"    android:orientation="vertical" >    <ImageView        android:id="@+id/audio_image"        android:layout_width="450dp"        android:layout_height="255dp"        android:layout_x="1305dp"        android:layout_y="285dp" />		    <TextView        android:id="@+id/audio_artist"        android:layout_width="600dp"        android:layout_height="36dp"        android:layout_x="1317dp"        android:layout_y="582dp"        android:gravity="left|center_vertical"        android:textColor="@color/black"        android:textSize="27sp" />    <TextView        android:id="@+id/audio_album"        android:layout_width="600dp"        android:layout_height="36dp"        android:layout_x="1359dp"        android:layout_y="618dp"        android:gravity="left|center_vertical"        android:textColor="@color/black"        android:textSize="27sp" />    <TextView        android:id="@+id/audio_path"        android:layout_width="352dp"        android:layout_height="36dp"        android:layout_x="128dp"        android:layout_y="604dp"        android:shadowColor="#aaaaaa"        android:singleLine="true"        android:textColor="@color/black"        android:textSize="18dp"        android:visibility="gone" />    <ProgressBar        android:id="@+id/audio_progress"        style="?android:attr/progressBarStyleHorizontal"        android:layout_width="495dp"        android:layout_height="12dp"        android:layout_x="1290dp"        android:layout_y="735dp" />    <TextView        android:id="@+id/audio_hasplayed"        android:layout_width="171dp"        android:layout_height="24dp"        android:layout_x="1464dp"        android:layout_y="780dp"        android:textColor="#fcb913"        android:textSize="16dp" />   <ListView        android:id="@+id/audio_list"        android:layout_width="1005dp"        android:layout_height="828dp"        android:layout_x="135dp"        android:layout_y="186dp"        android:dividerHeight="3dp"        android:listSelector="@drawable/selector_list_background">    </ListView>    <ImageView        android:id="@+id/imageView1"        android:layout_width="72dp"        android:layout_height="23dp"        android:layout_x="24dp"        android:layout_y="27dp"        android:src="@drawable/image_mbw_media_menu_music_icon" />    <ImageView        android:id="@+id/imgPath"        android:layout_width="27dp"        android:layout_height="27dp"        android:layout_x="129dp"        android:layout_y="126dp"        android:src="@drawable/list_common_icon_folder" />    <TextView        android:id="@+id/textView1"        android:layout_width="153dp"        android:layout_height="57dp"        android:layout_x="70dp"        android:layout_y="36dp"        android:text="@string/title_music"        android:textColor="@color/black"        android:textSize="30dp" />    <TextView        android:id="@+id/music_path_top"        android:layout_width="450dp"        android:layout_height="27dp"        android:layout_x="177dp"        android:layout_y="126dp"        android:shadowColor="#aaaaaa"        android:singleLine="true"        android:textColor="@color/black"        android:textSize="18dp" />    <TextView        android:id="@+id/totaltxt"        android:layout_width="150dp"        android:layout_height="27dp"        android:layout_x="981dp"        android:layout_y="126dp"        android:shadowColor="#aaaaaa"        android:singleLine="true"        android:text="@string/totaltime"        android:textColor="@color/black"        android:textSize="18dp" />    <TextView        android:id="@+id/audio_duration_tag"        android:layout_width="24dp"        android:layout_height="21dp"        android:layout_x="1530dp"        android:layout_y="780dp"        android:gravity="center"        android:text="@string/gapline"        android:textColor="@color/black"        android:textSize="16dp" />    <TextView        android:id="@+id/audio_duration"        android:layout_width="171dp"        android:layout_height="21dp"        android:layout_x="1551dp"        android:layout_y="780dp"        android:textColor="@color/black"        android:textSize="16dp" />    <ImageButton        android:id="@+id/imgRepeat"        android:layout_width="45dp"        android:layout_height="45dp"        android:layout_x="1722dp"        android:layout_y="780dp"        android:background="#00000000"        android:src="@drawable/av_repeat_icon" />    <TextView        android:id="@+id/TopIndexInfo_audio"        android:layout_width="228dp"        android:layout_height="60dp"        android:layout_x="1101dp"        android:layout_y="117dp"        android:gravity="center"        android:shadowColor="#aaaaaa"        android:textColor="@color/black"        android:textSize="30dp" /></AbsoluteLayout>