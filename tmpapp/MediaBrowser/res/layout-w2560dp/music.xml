<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/list_common_bg"    android:orientation="vertical" ><RelativeLayout        android:layout_width="match_parent"        android:layout_height="match_parent" >    <LinearLayout        android:id="@+id/bg_bottom"        android:layout_width="2560dp"        android:layout_height="92dp"        android:layout_marginLeft="0dp"        android:layout_marginTop="1348dp"        android:background="@drawable/list_common_bg_bottom" />    <ImageView        android:id="@+id/audio_image"        android:layout_width="600dp"        android:layout_height="340dp"        android:layout_marginLeft="1740dp"        android:layout_marginTop="380dp"        android:scaleType="fitCenter" />    <Progressbar        android:id="@+id/loadingIcon"        style="@android:attr/progressBarStyleSmall"        android:layout_width="72dp"        android:layout_height="72dp"        android:layout_marginLeft="2412dp"        android:layout_marginTop="72dp"        android:indeterminateDrawable="@anim/video_anim" />		    <TextView        android:id="@+id/audio_artist"        android:layout_width="588dp"        android:layout_height="48dp"        android:layout_marginLeft="1756dp"        android:layout_marginTop="776dp"        android:ellipsize="end"        android:singleLine="true"        android:gravity="left|center_vertical"        android:textColor="#0f265f"        android:textSize="36sp" />    <TextView        android:id="@+id/audio_album"        android:layout_width="588dp"        android:layout_height="48dp"        android:layout_marginLeft="1756dp"        android:layout_marginTop="824dp"        android:ellipsize="end"        android:singleLine="true"        android:gravity="left|center_vertical"        android:textColor="#0f265f"        android:textSize="36sp" />    <TextView        android:id="@+id/audio_path"        android:layout_width="704dp"        android:layout_height="72dp"        android:layout_marginLeft="256dp"        android:layout_marginTop="1208dp"        android:shadowColor="#aaaaaa"        android:textColor="#0f265f"        android:textSize="36dp"         android:visibility="gone"        android:singleLine="true"/>    <ProgressBar        android:id="@+id/audio_progress"        style="?android:attr/progressBarStyleHorizontal"        android:layout_width="668dp"        android:layout_height="16dp"        android:layout_marginLeft="1720dp"        android:layout_marginTop="980dp"        android:progressDrawable="@drawable/listprocess" />    <TextView        android:id="@+id/audio_hasplayed"        android:layout_width="228dp"        android:layout_height="32dp"        android:layout_marginLeft="1952dp"        android:layout_marginTop="1040dp"        android:textColor="#0f265f"        android:textSize="26dp" />    <ListView        android:id="@+id/audio_list"        android:layout_width="1340dp"        android:layout_height="1108dp"        android:layout_marginLeft="180dp"        android:layout_marginTop="240dp"        android:dividerHeight="1.34dp"        android:listSelector="@drawable/selector_list_background"        android:scrollbars="none" >    </ListView>    <ImageView        android:id="@+id/page_up"        android:layout_width="40dp"        android:layout_height="40dp"        android:layout_marginLeft="1576dp"        android:layout_marginTop="264dp"        android:scaleType="fitCenter"        android:src="@drawable/list_common_icon_up" />   	<ImageView   	    android:id="@+id/page_down"   	    android:layout_width="40dp"   	    android:layout_height="40dp"   	    android:layout_marginLeft="1576dp"   	    android:layout_marginTop="1276dp"   	    android:scaleType="fitCenter"   	    android:src="@drawable/list_common_icon_down" />    <ImageView        android:id="@+id/imageView1"        android:layout_width="96dp"        android:layout_height="96dp"        android:layout_marginLeft="32dp"        android:layout_marginTop="36dp"        android:src="@drawable/media_select_music" />    <ImageView        android:id="@+id/imgPath"        android:layout_width="36dp"        android:layout_height="36dp"        android:layout_marginLeft="172dp"        android:layout_marginTop="152dp"        android:src="@drawable/list_common_icon_folder" />    <TextView        android:id="@+id/textView1"        android:layout_width="204dp"        android:layout_height="76dp"        android:layout_marginLeft="140dp"        android:layout_marginTop="48dp"        android:text="@string/title_music"        android:textColor="#0f265f"        android:textSize="60dp" />    <TextView        android:id="@+id/music_path_top"        android:layout_width="960dp"        android:layout_height="36dp"        android:layout_marginLeft="236dp"        android:layout_marginTop="164dp"        android:shadowColor="#aaaaaa"        android:ellipsize="end"        android:singleLine="true"        android:textColor="#0f265f"        android:textSize="32dp" />    <TextView        android:id="@+id/totaltxt"        android:layout_width="200dp"        android:layout_height="36dp"        android:layout_marginLeft="1308dp"        android:layout_marginTop="164dp"        android:shadowColor="#aaaaaa"        android:singleLine="true"        android:text="@string/totaltime"        android:textColor="#0f265f"        android:textSize="32dp" />	<ImageView        android:id="@+id/audio_list_icon_line"        android:layout_width="1360dp"        android:layout_height="wrap_content"        android:layout_marginLeft="180dp"        android:layout_marginTop="236dp"        android:src="@drawable/list_common_icon_line" />    <TextView        android:id="@+id/audio_duration_tag"        android:layout_width="32dp"        android:layout_height="32dp"        android:layout_marginLeft="2040dp"        android:layout_marginTop="1040dp"        android:gravity="center"        android:text="@string/gapline"        android:textColor="#0f265f"        android:textSize="26dp" />    <TextView        android:id="@+id/audio_duration"        android:layout_width="228dp"        android:layout_height="32dp"        android:layout_marginLeft="2068dp"        android:layout_marginTop="1040dp"        android:textColor="#0f265f"        android:textSize="26dp" />        <ImageButton        android:id="@+id/imgRepeat"        android:layout_width="60dp"        android:layout_height="60dp"        android:layout_marginLeft="2296dp"        android:layout_marginTop="1040dp"        android:background="#00000000"        android:scaleType="fitCenter"        android:src="@drawable/av_repeat_icon" />    <TextView        android:id="@+id/TopIndexInfo_audio"        android:layout_width="304dp"        android:layout_height="80dp"        android:layout_marginLeft="1468dp"        android:layout_marginTop="156dp"        android:gravity="center"        android:shadowColor="#aaaaaa"        android:textColor="#0f265f"        android:textSize="40dp" />        </RelativeLayout></LinearLayout>